<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="catelera">

    <div id="divs" class="demo row">
        <div class="col-xs-12 col-sm-10 col-sm-offset-1">
            
        <c:forEach begin="1" end="50" var="i">
            <div id="p${i}" class="col-lg-3 col-md-4 col-sm-6 col-xs-12 thumb">
            	<c:if test="${ not empty param.acciones}">
            	<div class="publicacion-header pull-right">
                    <div class="pull-right">
                        <a href="<c:url value="/publicacion/modificar.jsp"/>">
                            <span class="glyphicon glyphicon-pencil"></span>
                        </a>
                        <a href="#">
                            <span class="glyphicon glyphicon-trash"></span>
                        </a>
                    </div>
                </div>
                </c:if>
                <div class="publicacion-body">
                    
                    <a data-toggle="modal" data-target="#imagen-pop" class="thumbnail" href="#">
                        <img src="<c:url value="/assets/img/ejemplo.png"/>" class="img-responsive" alt="ejemplo de publicación">
                    </a>
                </div>
            </div>
        </c:forEach>
        </div>
    </div>
</div>
<div id="black" style="margin: auto;"></div>


<div class="modal fade" id="imagen-pop" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Publicación</h4>
            </div>
            <div class="modal-body">
                <img src="<c:url value="/assets/img/ejemplo.png"/>" class="publicacion" alt="ejemplo de publicación">
                <div>
                    <div class="msg-wrap">


                        <div class="media msg ">
                            <a class="pull-left" href="#">
                                <img class="media-object" data-src="holder.js/64x64" alt="64x64" style="width: 32px; height: 32px;" src="data:image/png;base64,">
                            </a>
                            <div class="media-body">
                                <small class="pull-right time"><i class="fa fa-clock-o"></i> 12:10am</small>
                                <h5 class="media-heading">Laura</h5>
                                <small class="col-lg-10">No me gusta tu publicacion</small>
                            </div>
                        </div>
                        <div class="alert alert-info msg-date">
                            <strong>Hoy</strong>
                        </div>
                        <div class="media msg">
                            <a class="pull-left" href="#">
                                <img class="media-object" data-src="holder.js/64x64" alt="64x64" style="width: 32px; height: 32px;" src="data:image/png;base64,">
                            </a>
                            <div class="media-body">
                                <small class="pull-right time"><i class="fa fa-clock-o"></i> 12:10am</small>

                                <h5 class="media-heading">Pablo Araujo</h5>
                                <small class="col-lg-10">Hola todo bien</small>
                            </div>
                        </div>
                        <div class="media msg">
                            <a class="pull-left" href="#">
                                <img class="media-object" data-src="holder.js/64x64" alt="64x64" style="width: 32px; height: 32px;" src="data:image/png;base64,">
                            </a>
                            <div class="media-body">
                                <small class="pull-right time"><i class="fa fa-clock-o"></i> 12:10am</small>

                                <h5 class="media-heading">TTPS JAVA</h5>
                                <small class="col-lg-10">Hola todo bien</small>
                            </div>
                        </div>

                        <div class="media msg">
                            <a class="pull-left" href="#">
                                <img class="media-object" data-src="holder.js/64x64" alt="64x64" style="width: 32px; height: 32px;" src="data:image/png;base64,">
                            </a>
                            <div class="media-body">
                                <small class="pull-right time"><i class="fa fa-clock-o"></i> 12:10am</small>
                                <h5 class="media-heading">TTPS JAVA</h5>

                                <small class="col-lg-10">Hola todo bien</small>
                            </div>
                        </div>

                    </div>

                    <div class="send-wrap ">

                        <textarea class="form-control send-message" rows="3" placeholder="Escribir un comentario .."></textarea>
                    </div>             
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default"
                        data-dismiss="modal">
                            Salir
                </button>
                <a type="button" class="btn btn-default"
                        data-dismiss="modal" role="button">
                            Enviar mensaje
                </a>
            </div>
        </div>
    </div>
</div>